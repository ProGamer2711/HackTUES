<% JSON.parse(rocketData).forEach(data => { %>
<div class="rocket">
	<div class="rocket-text">
		<h3><%= data.name %></h3>

		<p>Launch Service Provider: <%= data.lsp %></p>
		<p>Date Of Launch: <%= data.date %></p>
		<p>Location: <%= data.location %></p>

		<p>Status: <%= data.status %></p>
		<p>Status Description: <%= data.statusDescription %></p>
	</div>
	<div class="rocket-image">
		<div
			class="rocket-image-wrapper"
			style="
				background-image: url('<%= data.image %>');
				background-size: cover;
				background-position: center;
			"
		></div>
	</div>
</div>
<% }) %>
